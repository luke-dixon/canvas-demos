  "bugs": {
    "url": "https://github.com/luke-dixon/canvas-demos/issues"
  },
  "homepage": "https://github.com/luke-dixon/canvas-demos#readme",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "html-webpack-plugin": "^5.5.0",
    "jasmine": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^7.8.3",
    "style-loader": "^3.3.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
