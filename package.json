    "test": "karma start --single-run",
    "test-watch": "karma start --auto-watch",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luke-dixon/canvas-demos.git"
  },
  "author": "Luke Dixon",
